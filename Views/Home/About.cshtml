@model Message
@{
    ViewData["Title"] = "About";
}

<h1>
    @Model.Title
</h1>
<div class="alert alert-primary" role="alert">
    @Model.Descritpion
</div>
<p>Tags:</p>
<ul>
    @foreach (var tag in Model.Tags)
    {
        <li>@tag</li>
    }
</ul>


<div class="container text-center">
    <div class="row">
        <div class="col-xl-4 border">
            Column
        </div>
        <div class="col-xl-1 border">
            Column
        </div>
        <div class="col-xl-1 border">
            Column
        </div>
        <div class="col-xl-6 border">
            Column
        </div>
    </div>
</div>

